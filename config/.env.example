# Discord Bot Configuration
# Copy this file to .env and fill in your actual values

# Discord bot authentication token (REQUIRED)
# Get this from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Logging level (optional, default: INFO)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Base URL for image hosting (REQUIRED)
# Must use HTTPS protocol
# Example: https://heckatron.xyz/images/
BASE_IMAGE_URL=https://heckatron.xyz/images/

# Path to servers.json config file (optional, default: config/servers.json)
# SERVERS_CONFIG_PATH=config/servers.json
